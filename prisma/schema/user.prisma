enum UserRole {
      ADMIN
      USER
        @@schema("auth")
    }

enum UserStatus {
  ACTIVE
  INACTIVE

  @@schema("auth")
}

model User {
  id    String @id @default(uuid())
  username String  @unique
  password String 
  role UserRole @default(USER)
  status UserStatus @default(ACTIVE)
  
  @@schema("auth")
}